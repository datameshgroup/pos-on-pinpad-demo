<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="au.com.dmg.terminalposdemo">
    <uses-permission android:name="android.permission.CAMERA"/>
    <application
        android:name="au.com.dmg.terminalposdemo.GlobalClass"
        android:allowBackup="true"
        android:extractNativeLibs="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TerminalPOSDemo"
        tools:targetApi="31">
        <activity
            android:name="au.com.dmg.terminalposdemo.ActivityResult"
            android:exported="false" />
        <activity
            android:name="au.com.dmg.terminalposdemo.ActivityPayment"
            android:exported="false" />
        <activity
            android:name="au.com.dmg.terminalposdemo.ActivityRequests"
            android:exported="false" />
        <activity
            android:name="au.com.dmg.terminalposdemo.ActivitySatellite"
            android:exported="false" />
        <activity
            android:name="au.com.dmg.terminalposdemo.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>